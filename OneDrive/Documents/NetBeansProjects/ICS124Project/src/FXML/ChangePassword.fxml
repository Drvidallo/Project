<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="287.0" prefWidth="487.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ChangePasswordController">
   <children>
      <Pane prefHeight="67.0" prefWidth="487.0" style="-fx-background-color: #ffdc65;">
         <children>
            <Label layoutX="24.0" layoutY="25.0" text="Change Password">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <JFXButton fx:id="saveBtn" layoutX="32.0" layoutY="208.0" onAction="#saveBtnClicked" prefHeight="38.0" prefWidth="147.0" style="-fx-background-color: #000;" text="Save" textFill="#fcfcfc">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="cancelBtn" layoutX="262.0" layoutY="208.0" onAction="#cancelBtnClicked" prefHeight="38.0" prefWidth="147.0" style="-fx-background-color: #000;" text="Cancel" textFill="#fcfcfc">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
      <JFXPasswordField fx:id="oldPassword" labelFloat="true" layoutX="38.0" layoutY="112.0" promptText="Old Password" />
      <JFXPasswordField fx:id="newPassword" labelFloat="true" layoutX="262.0" layoutY="112.0" promptText="New Password" />
   </children>
</AnchorPane>
